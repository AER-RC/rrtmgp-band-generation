#

OBJPATH = ../objdir

SRCPATH = .

VPATH = $(SRCPATH): $(OBJPATH)

OBJ1 = \
	kdis_2sort_l 
OBJ2 = \
	write_tape5_csh \
	write_tape5_nc_csh 


OBJ3 = \
	write_data_eta \
	write_data_multiple_csh

all: makedir	$(OBJ1) $(OBJ2) $(OBJ3)

% :%.f 
	pgf90  $< -o $(OBJPATH)/$@


.SILENT:

makedir :
	if [ ! -d $(OBJPATH) ] ; \
	then \
	mkdir $(OBJPATH) ; \
	fi

